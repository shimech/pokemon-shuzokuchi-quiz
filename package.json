{
  "name": "pokemon-shuzokuchi-quiz-neo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "migrate": "sh ./bin/migrate.sh",
    "prepare": "husky install",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{ts,tsx} --fix",
    "format": "prettier --write \"./src/**/*.ts\" \"./src/**/*.tsx\""
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^15.14.0",
    "@types/react": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.2",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format",
      "yarn lint:fix"
    ]
  }
}
